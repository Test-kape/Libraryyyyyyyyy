{
  "name": "electron-pdf-reader",
  "version": "1.0.0",
  "description": "A simple PDF and EPUB reader built with Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "keywords": [
    "electron",
    "pdf",
    "epub",
    "reader"
  ],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.electron.pdfreader",
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "electron": "^28.1.0",
    "epub": "^1.3.0",
    "epub-metadata": "^0.2.0",
    "epubjs": "^0.3.93",
    "jszip": "^3.10.1",
    "pdf.js-extract": "^0.2.1",
    "pdfjs-dist": "^2.16.105"
  },
  "devDependencies": {
    "electron-builder": "^24.9.1"
  },
  "type": "commonjs"
}
